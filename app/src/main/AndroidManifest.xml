<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kk.kktools">

    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@drawable/ic_launcher_background" android:largeHeap="true"
        android:label="@string/app_name"
        android:supportsRtl="true" android:resizeableActivity="true" android:maxAspectRatio="2.1"
        android:theme="@style/AppTheme">
        <activity
            android:name="kk.kktools.MainActivity"  android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>